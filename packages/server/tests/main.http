    { "code": "SH000001", "timestamp": "2025-04-12 10:00:00", "type": "buy", "price": 100, "volume": 100 },
    { "code": "SH000001", "timestamp": "2025-04-13 10:00:00", "type": "sell", "price": 100, "volume": 100 }
]

