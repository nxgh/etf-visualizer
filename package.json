  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "pnpm run dev:server && pnpm run dev:web",
    "dev:server": "pnpm --filter @etf-visualizer/server run dev",
    "dev:web": "pnpm --filter @etf-visualizer/frontend run dev",
    "build": "pnpm --filter '@etf-visualizer/*' run build",
    "build:server": "pnpm --filter @etf-visualizer/server run build",
    "build:db": "pnpm --filter @etf-visualizer/database run build",
    "build:shared": "pnpm --filter @etf-visualizer/shared run build",
    "build:spider": "pnpm --filter @etf-visualizer/spider run build",
    "build:web": "pnpm --filter @etf-visualizer/frontend run build",
    "build:package": "pnpm run build:shared && pnpm run build:spider && pnpm run build:db"
  },
